<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

	<description><![CDATA[My ZK Application]]></description>
	<display-name>java-client-demo</display-name>


	<filter>
		<filter-name>DemoFilter</filter-name>
		<filter-class>keikai.demo.DemoFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>DemoFilter</filter-name>
		<url-pattern>/javaClientDemo.jsp</url-pattern>
	</filter-mapping>

	<!-- register ZK servlets with annotations -->
	
	<welcome-file-list>
		<welcome-file>javaClientDemo.html</welcome-file>
	</welcome-file-list>
</web-app>
